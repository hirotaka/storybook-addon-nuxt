{
  "name": "storybook-addon-nuxt",
  "version": "0.0.0-development",
  "type": "module",
  "author": "hirotaka",
  "license": "MIT",
  "description": "A no config Storybook addon that makes nuxtjs features just work in Storybook",
  "keywords": [
    "storybook",
    "addon",
    "nuxt",
    "nuxtjs"
  ],
  "homepage": "https://github.com/hirotaka/storybook-addon-nuxt#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/hirotaka/storybook-addon-nuxt.git"
  },
  "bugs": {
    "url": "https://github.com/hirotaka/storybook-addon-nuxt/issues"
  },
  "storybook": {
    "displayName": "Nuxt + Storybook"
  },
  "main": "dist/preset.js",
  "files": [
    "dist",
    "runtime"
  ],
  "scripts": {
    "watch": "tsc --watch",
    "build": "npm run clean && tsc",
    "clean": "tsc --build --clean",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "format:all": "npm run format .",
    "lint": "eslint --fix",
    "lint:all": "npm run lint .",
    "test:all": "npm run lint:all && npm run type-check",
    "prepublishOnly": "npm run build",
    "release": "semantic-release",
    "prepack": "npm run build"
  },
  "dependencies": {
    "loader-utils": "^3.2.1",
    "resolve-url-loader": "^5.0.0",
    "semver": "^7.3.8",
    "tsconfig-paths": "^4.1.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.0.11",
    "@storybook/addons": "^7.0.11",
    "@storybook/core-common": "^7.0.11",
    "@storybook/vue3-vite": "^7.0.11",
    "@types/loader-utils": "^2.0.3",
    "@types/react": "^18.0.28",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-typescript": "^3.0.0",
    "lint-staged": "^13.1.2",
    "nuxt": "^3.4.3",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "semantic-release": "^21.0.2",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@storybook/addon-actions": "^7.0.0",
    "@storybook/addons": "^7.0.0",
    "nuxt": "^3.0.0",
    "vue": "^3.0.0"
  }
}
